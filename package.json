{
  "name": "managr",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "configure": "babel-node --presets @babel/env src/configure.js",
    "build": "babel src --out-dir dist",
    "serve": "yarn configure && node dist/index.js",
    "start": "yarn configure && nodemon --ignore src/config.json --exec \"babel-node src/index.js\""
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.4",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.18.5"
  }
}
